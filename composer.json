{
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Yohan Giarelli",
            "email": "yohan@giarel.li"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "doctrine/doctrine-bundle": "^2.16.2",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.2",
        "easycorp/easyadmin-bundle": "^4.25.1",
        "knpuniversity/oauth2-client-bundle": "^2.19.0",
        "league/oauth2-google": "^4.0.1",
        "runtime/frankenphp-symfony": "^0.2.0",
        "sensiolabs/gotenberg-bundle": "*",
        "symfony/console": "7.4.*",
        "symfony/dotenv": "7.4.*",
        "symfony/flex": "^2.8.2",
        "symfony/framework-bundle": "7.4.*",
        "symfony/http-client": "7.4.*",
        "symfony/mailer": "7.4.*",
        "symfony/mailgun-mailer": "7.4.*",
        "symfony/monolog-bundle": "^3.10.0",
        "symfony/runtime": "7.4.*",
        "symfony/scaleway-mailer": "7.4.*",
        "symfony/stimulus-bundle": "^2.30.0",
        "symfony/translation": "7.4.*",
        "symfony/twig-bundle": "7.4.*",
        "symfony/uid": "7.4.*",
        "symfony/ux-chartjs": "^2.30.0",
        "symfony/webpack-encore-bundle": "^2.3.0",
        "symfony/yaml": "7.4.*",
        "symfonycasts/reset-password-bundle": "^1.23.2",
        "twig/cache-extra": "^3.21.0",
        "twig/extra-bundle": "^3.21.0",
        "twig/intl-extra": "^3.21.0",
        "twig/twig": "^3.21.1",
        "yohang/calendr": "^3.0.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.3",
        "friendsofphp/php-cs-fixer": "^3.88",
        "phpunit/phpunit": "^12.4.0",
        "rector/rector": "^2.1",
        "symfony/browser-kit": "7.4.*",
        "symfony/css-selector": "7.4.*",
        "symfony/debug-bundle": "7.4.*",
        "symfony/maker-bundle": "^1.50",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "7.4.*",
        "symfony/web-profiler-bundle": "7.4.*",
        "vimeo/psalm": "^6.13.1",
        "vincentlanglet/twig-cs-fixer": "^3.10",
        "zenstruck/foundry": "^2.7"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "reset": [
            "php bin/console cache:pool:clear cache.adapter.auto",
            "php bin/console doctrine:schema:drop --force --full-database",
            "php bin/console doctrine:schema:create",
            "php bin/console doctrine:migration:sync-metadata-storage",
            "php bin/console doctrine:migrations:version --add --all",
            "php bin/console foundry:load-fixtures main --no-interaction --append"
        ],
        "reset-test": [
            "php bin/console -etest cache:pool:clear cache.adapter.auto",
            "php bin/console -etest doctrine:database:drop -f || true",
            "php bin/console -etest doctrine:database:create || true",
            "php bin/console -etest doctrine:schema:create",
            "php bin/console -etest doctrine:migration:sync-metadata-storage",
            "php bin/console -etest doctrine:migrations:version --add --all",
            "php bin/console -etest foundry:load-fixtures main --no-interaction --append",
            "php bin/console -etest foundry:load-fixtures dev --no-interaction --append"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*",
            "docker": false
        }
    }
}
